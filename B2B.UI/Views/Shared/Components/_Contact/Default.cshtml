@model List<ResultContactDto>

<div class="col-md-5 mr-auto p-5">
    <h2 class="font-weight-bold text-white">Bizimle İletişime Geçin</h2>
    @foreach (var item in Model)
{

        <p class="mb-5">
            @item.ContactDesc
        </p>

        <ul class="list-unstyled pl-md-5 mb-5 text-white">
            <li class="d-flex text-black mb-2">
                <span class="mr-3"><span class="icon-map"></span></span> @item.ContactAdress
            </li>
            <li class="d-flex text-black mb-2"><span class="mr-3"><span class="icon-phone"></span></span> @item.ContactPhone1</li>
  
            <li class="d-flex text-black"><span class="mr-3"><span class="icon-envelope-o"></span></span> @item.ContactMail1 </li>
           
        </ul>
 
}
</div>